{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.0.6",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.1.2",
    "antd": "^4.1.5",
    "axios": "^0.19.2",
    "jsencrypt": "^3.0.0-rc.1",
    "lint-staged": "^10.0.7",
    "moment": "^2.24.0",
    "prettier": "^1.19.1",
    "rc-texty": "^0.2.0",
    "rc-tween-one": "^2.6.8",
    "react": "^16.12.0",
    "react-ace": "^8.1.0",
    "react-dom": "^16.12.0",
    "redux": "^4.0.5",
    "umi": "^3.1.2",
    "yorkie": "^2.0.0"
  }
}
